<!DOCTYPE html>
<html lang="be">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Lab 15</title>
  <link rel="stylesheet" href="/styles/style.css">
</head>

<body>
  <main>
    <ul class="list-container">
      {{#each data}}
      <li>{{> record}}</li>
      {{/each}}
    </ul>
    {{#if renderGet}}
    {{> getPanel data=data}}
    {{/if}}
    {{#if renderAdd}}
    {{> addPanel data=data}}
    {{/if}}
    {{#if renderUpdate}}
    {{> updatePanel data=data record=record}}
    {{/if}}
  </main>
</body>

</html>